  // U74589834
function Car(make, speed) {
  this.make = make;
  this.speed = speed;
 this.accelerate = function() {
    this.speed += 10;
    console.log(this.speed);
  };
this.brake = function() {
    this.speed -= 5;
    console.log(this.speed);
  };
}
var car1 = new Car('BMW', 120);
var car2 = new Car('Mercedes', 95);
car1.accelerate();
car1.brake();
car2.accelerate();
car2.brake();
